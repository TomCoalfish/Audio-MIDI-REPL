@prefix doap: 	<http://usefulinc.com/ns/doap#> .
@prefix foaf: 	<http://xmlns.com/foaf/0.1/> .
@prefix lv2: 	<http://lv2plug.in/ns/lv2core#> .
@prefix rdf: 	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: 	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix ll: 	<http://ll-plugins.nongnu.org/lv2/namespace#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#> .

<http://github.com/blablack/ams-lv2/ad_2/gui>
  a guiext:GtkUI ;
  guiext:binary <ad_2_gui.so> .

<http://github.com/blablack/ams-lv2/ad_2>
	a lv2:Plugin, lv2:SpectralPlugin ;
	lv2:project <http://github.com/blablack/ams-lv2/> ;
	doap:name "AMS Analog Driver - 2 Outputs" ;
	doap:license <http://opensource.org/licenses/isc> ;
	ll:pegName "p" ;
	guiext:ui <http://github.com/blablack/ams-lv2/ad_2/gui> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port 
		[
		a lv2:InputPort, lv2:CVPort ;
		lv2:index 0 ; 
		lv2:symbol "cvIn" ;
		lv2:name "CV In" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 1 ; 
		lv2:symbol "detuneAmplitude" ;
		lv2:name "Detune Amplitude";
		lv2:default 0.005 ;
		lv2:minimum 0 ;
		lv2:maximum 0.084 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ; 
		lv2:symbol "detuneModulation" ;
		lv2:name "Detune Modulation";
		lv2:default 0.01 ;
		lv2:minimum 0.01 ;
		lv2:maximum 1 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ; 
		lv2:symbol "detuneRate" ;
		lv2:name "Detune Rate";
		lv2:default 1 ;
		lv2:minimum 0.01 ;
		lv2:maximum 10 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ; 
		lv2:symbol "driftAmplitude" ;
		lv2:name "Drift Amplitude";
		lv2:default 0.005 ;
		lv2:minimum 0 ;
		lv2:maximum 0.084 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ; 
		lv2:symbol "driftModulation" ;
		lv2:name "Drift Modulation";
		lv2:default 0.01 ;
		lv2:minimum 0.01 ;
		lv2:maximum 1 ;
	] ,	[
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ; 
		lv2:symbol "driftRate" ;
		lv2:name "Drift Rate";
		lv2:default 3 ;
		lv2:minimum 0.01 ;
		lv2:maximum 10 ;
	] , 	[
		a lv2:OutputPort, lv2:CVPort ;
		lv2:index 7 ; 
		lv2:symbol "cvOut1" ;
		lv2:name "CV Out 1" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
	        lv2:maximum 1.0 ;
	] , 	[
		a lv2:OutputPort, lv2:CVPort ;
		lv2:index 8 ; 
		lv2:symbol "cvOut2" ;
		lv2:name "CV Out 2" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
        	lv2:maximum 1.0 ;
	].
